<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="NoUnderlineEditText">
        <item name="android:background">@null</item>
    </style>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:navigationBarColor">@color/black</item>
    </style>
    <style name="SquareEditTextStyle" parent="Widget.AppCompat.EditText">
        <item name="android:background">@drawable/square_edittext_background</item>
    </style>
    <style name="CustomDialog" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
</resources>